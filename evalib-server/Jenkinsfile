import groovy.json.JsonOutput

node {
    checkout scm
	
	stages {
		stage('Build') {
			steps {
				echo 'Building...'
                echo "Running ${env.BUILD_ID} on ${env.JENKINS_URL}"
			}
		}
		stage('Test') {
			steps {
				echo 'Testing...'
			}
		}
		stage('Publish') {
			steps {
				echo 'Publishing...'
			}
		}
   }
   post {
		always {
			echo 'post section...'
		}
		failure {
			echo 'failure...'
		}
   }
}